%body
	#menu
		%a.start(href="#game") start
		%a.settings(href="#settings") Instellingen
	#game
		.dashboard
			#timer 10.0
			#score 0
		.som
			#times
			&times;
			#tafel
		.answer
			%form#form
				%input#answer(type="number" name="answer" required)
	#result
		%a.start(href="#game") nog eens
		%a.settings(href="#settings") Instellingen
	#settings
		%h2 Welke tafels?
		%input.tafel#tafel-1(type="checkbox" name="tafels[]" value="1")
		%label(for="tafel-1") 1
		%input.tafel#tafel-2(type="checkbox" name="tafels[]" value="2")
		%label(for="tafel-2") 2
		%input.tafel#tafel-3(type="checkbox" name="tafels[]" value="3")
		%label(for="tafel-3") 3
		%input.tafel#tafel-4(type="checkbox" name="tafels[]" value="4")
		%label(for="tafel-4") 4
		%input.tafel#tafel-5(type="checkbox" name="tafels[]" value="5")
		%label(for="tafel-5") 5
		%input.tafel#tafel-6(type="checkbox" name="tafels[]" value="6")
		%label(for="tafel-6") 6
		%input.tafel#tafel-7(type="checkbox" name="tafels[]" value="7")
		%label(for="tafel-7") 7
		%input.tafel#tafel-8(type="checkbox" name="tafels[]" value="8")
		%label(for="tafel-8") 8
		%input.tafel#tafel-9(type="checkbox" name="tafels[]" value="9")
		%label(for="tafel-9") 9
		%input.tafel#tafel-10(type="checkbox" name="tafels[]" value="10")
		%label(for="tafel-10") 10
		%a.start(href="#game") start
		
